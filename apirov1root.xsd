                    <xs:sequence>
                        <xs:element name="groupTag" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="metaData" type="ap:genMetaData" minOccurs="0"/>
