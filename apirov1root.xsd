                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="dataPoint" type="ap:genSchemaDataItem" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="schemaAppliedProcessors" type="ap:genSchemaProcessors" minOccurs="0"/>
                    <xs:element name="dataPointCollections" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="dataPointCollection" type="ap:genScriptEntity" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="alerts" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
