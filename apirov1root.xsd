                    </xs:element>
                    <xs:element name="backingLake" type="ap:genScriptEntity" minOccurs="0"/>
                </xs:all>
                <xs:attribute name="name" type="xs:string"/>
                <xs:attribute name="defBacked" type="xs:boolean"/>
                <xs:attribute name="historical" type="xs:boolean"/>
                <xs:attribute name="extendsFrom" type="xs:string"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="genSchemaDataItem">
        <xs:complexContent>
            <xs:extension base="ap:genBase">
                <xs:all>
                    <xs:element name="description" type="xs:string" minOccurs="0"/>
