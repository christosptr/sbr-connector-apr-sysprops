                    <xs:element name="consolidationAlgorithm" type="ap:genScriptPipelineEntity" minOccurs="0"/>
                    <xs:element name="rawDPValidators" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="rawDPValidator" type="ap:genScriptPipelineELBoundEntity" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="rawDPProcessors" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="rawDPProcessor" type="ap:genScriptPipelineEntity" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="consDPValidators" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="consDPValidator" type="ap:genScriptPipelineELBoundEntity" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="consDPProcessors" minOccurs="0">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="consDPProcessor" type="ap:genScriptPipelineEntity" minOccurs="0" maxOccurs="unbounded"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="pii" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="encrypted" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="encryptionKeyId" type="xs:string" minOccurs="0"/>
                    <xs:element name="ignoreChangeDetect" type="xs:boolean" minOccurs="0"/>
                </xs:all>
                <xs:attribute name="name" type="xs:string"/>
                <xs:attribute name="dataType" type="dataType"/>
                <xs:attribute name="extendsFrom" type="xs:string"/>
                <xs:attribute name="displayName" type="xs:string"/>
                <xs:attribute name="defaultValue" type="xs:string"/>
                <xs:attribute name="canEditValid" type="xs:boolean"/>
                <xs:attribute name="canEditViolated" type="xs:boolean"/>
            </xs:extension>
        </xs:complexContent>
