        </xs:all>
        <xs:attribute name="name" type="xs:string"/>
    </xs:complexType>

    <xs:complexType name="genUserMetaData">
        <xs:all>
            <xs:element name="userName" type="xs:string" minOccurs="0"/>
            <xs:element name="metaData" type="ap:genMetaData" minOccurs="0"/>
