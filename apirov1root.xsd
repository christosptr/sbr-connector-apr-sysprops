        <xs:attribute name="name" type="xs:string"/>
    </xs:complexType>

    <xs:complexType name="genExecutionDomain">
        <xs:complexContent>
            <xs:extension base="ap:genBase">
                <xs:all>
                    <xs:element name="description" type="xs:string" minOccurs="0"/>
