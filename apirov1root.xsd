    <xs:complexType name="genSchema">
        <xs:complexContent>
            <xs:extension base="ap:genBase">
                <xs:all>
                    <xs:element name="description" type="xs:string" minOccurs="0"/>
                    <xs:element name="abstract" type="xs:boolean" minOccurs="0"/>
                    <xs:element name="workingRetention" type="xs:string" minOccurs="0"/>
                    <xs:element name="historicalRetention" type="xs:string" minOccurs="0"/>
